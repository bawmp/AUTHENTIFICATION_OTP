{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/", "methods": ["GET"], "dest": "index.js" },
    { "src": "/auth/register", "methods": ["POST"], "dest": "index.js" },
    { "src": "/auth/login", "methods": ["POST"], "dest": "index.js" },
    { "src": "/auth/logout", "methods": ["POST"], "dest": "index.js" },
    { "src": "/auth/verify-otp", "methods": ["POST"], "dest": "index.js" },
    { "src": "/auth/new-otp", "methods": ["POST"], "dest": "index.js" }
  ]
}
